{
  "compilerOptions": {
    "noImplicitAny": true,
    "listFiles": true,
    "diagnostics": true,
    "jsx": "React",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "target": "es6"
  },
  "ignoreRules": {
      "TS2307": true
  },
  "exclude": [
    "build",
    "src",
    "dist",
    "node_modules"
  ]
}
